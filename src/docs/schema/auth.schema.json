{
  "SchemaAuth": {
    "type": "object",
    "properties": {
      "name": {
        "id": "string"
      },
      "username": {
        "type": "string"
      },
      "full_name": {
        "type": "string"
      },
      "email": {
        "type": "string"
      }
    }
  },
  "SchemaAuthRequest": {
    "type": "object",
    "properties": {
      "username": {
        "type": "string"
      },
      "full_name": {
        "type": "string"
      },
      "email": {
        "type": "string"
      },
      "password": {
        "type": "string"
      }
    }
  },
  "SchemaAuthLogin": {
    "type": "object",
    "properties": {
      "username": {
        "type": "string",
        "summary": "username or email"
      },
      "password": {
        "type": "string"
      }
    }
  },
  "SchemaAuthLoginConductor": {
    "type": "object",
    "properties": {
      "username": {
        "type": "string",
        "summary": "username"
      },
      "password": {
        "type": "string"
      },
      "role": {
        "type": "string",
        "summary": "Conductor,Auctioneer,Observer",
        "required": true,
        "enum": ["Conductor", "Auctioneer", "Observer"]
      }
    }
  },
  "SchemaAuthLoginResponse": {
    "type": "object",
    "properties": {
      "access_token": {
        "type": "string"
      },
      "refresh_token": {
        "type": "string"
      }
    }
  },
  "SchemaAuthOnlineAuctionLogin": {
    "type": "object",
    "properties": {
      "customer_no": {
        "type": "string"
      },
      "password": {
        "type": "string"
      }
    }
  }
}
