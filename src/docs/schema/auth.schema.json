{
  "SchemaAuthLogin": {
    "type": "object",
    "properties": {
      "email": {
        "type": "string",
        "summary": "Enter your email",
        "default": "admin@example.com"
      },
      "password": {
        "type": "string",
        "summary": "Enter your password",
        "default": "oracle123"
      },
      "security_question_answer": {
        "type": "string",
        "summary": "Enter your answare of the question",
        "default": "abcd"
      },
      "remember_me": {
        "type": "boolean",
        "default": false,
        "summary": "Check it to set remember credential"
      }
    }
  },
  "SchemaAuthForgotPassword": {
    "type": "object",
    "properties": {
      "email": {
        "type": "string"
      }
    }
  },
  "SchemaAuthVerifiedOTP": {
    "type": "object",
    "properties": {
      "email": {
        "type": "string"
      },
      "reset_token_code": {
        "type": "string",
        "default": "178729"
      }
    }
  },
  "SchemaAuthResetPassword": {
    "type": "object",
    "properties": {
      "email": {
        "type": "string"
      },
      "new_password": {
        "type": "string"
      },
      "confirm_password": {
        "type": "string"
      }
    }
  }
}
